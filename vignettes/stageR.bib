issn = {1471-2105},
journal = {BMC bioinformatics},
keywords = {Exploratory data analysis,Interactive visualization,Kallisto,RNA-Seq,Reanalysis,Reproducibility,Sequence read archive,Shiny,Sleuth},
month = {dec},
